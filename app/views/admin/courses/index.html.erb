<% provide(:title, 'All Courses') %>
<h1>All courses</h1>
<%= will_paginate %>
  <ul>
    <% @courses.each do |course| %>
      <li>
        <%= link_to course.name, admin_course_path(course) %>
        <%= link_to "", admin_course_path(course),
    method: :delete, data: {confirm: "You sure?"}, class: "glyphicon glyphicon-trash" %>
      </li>
    <% end %>
  </ul>
<%= will_paginate %>
